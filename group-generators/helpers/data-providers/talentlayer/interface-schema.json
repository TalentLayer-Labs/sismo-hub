{
  "name": "Talent Layer",
  "iconUrl": "https://www.talentlayer.org/assets/img/favicons/favicon-32x32.png",
  "providerClassName": "TalentLayerProvider",
  "functions": [
    {
      "name": "Get Talent Layer Users",
      "functionName": "getUsersWithTalentLayerId",
      "countFunctionName": "getTalentLayerUsersCountQuery",
      "description": "Returns all users that have minted a TalentLayer ID",
      "args": []
    },
    {
      "name": "Get Talent experienced in a topic",
      "functionName": "didWorkOnTopic",
      "countFunctionName": "getTalentLayerUsersCountQuery",
      "description": "Returns all users that already work at least a given number of times times on a given topic project on TalentLayer protocol",
      "args": [
        {
          "name": "topic",
          "argName": "topic",
          "type": "string",
          "example": "solidity",
          "description": "The selected topic"
        },
        {
          "name": "numberOfTimes",
          "argName": "numberOfTimes",
          "type": "number",
          "example": "3",
          "description": "The selected numberOfTimes"
        }
      ]
    },
    {
      "name": "Check if a User minimally earned an amount of a specific token",
      "functionName": "didUserMinimalEarnedOfToken",
      "countFunctionName": "didUserMinimalEarnedOfTokenCount",
      "description": "Check to see if a user earned more than a certain amount of a token in total",
      "args": [
        {
          "name": "userHandle",
          "argName": "userHandle",
          "type": "string",
          "example": "miguel",
          "description": "The handle of the user"
        },
        {
          "name": "minimumEarnings",
          "argName": "minimumEarnings",
          "type": "number",
          "example": "1000000",
          "description": "The minal amount the user must have earned"
        },
        {
          "name": "tokenSymbol",
          "argName": "tokenSymbol",
          "type": "string",
          "example": "MATIC",
          "description": "The symbol of the token"
        }
      ]
    }
  ]
}